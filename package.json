  "scripts": {
    "start": "node src/server.js",
    "test:api": "cross-env NODE_ENV=test mocha \"test/**/*.spec.js\" --timeout 30000 --grep \"^Auth & API flow$\"",
    "test:e2e": "cross-env NODE_ENV=test mocha \"test/**/*.spec.js\" --timeout 60000",
    "test:e2e:headed": "cross-env NODE_ENV=test HEADED=1 SLOWMO=250 DEBUG_E2E=1 mocha \"test/**/*.spec.js\" --timeout 120000",
